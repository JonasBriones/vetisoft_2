{
 "arrowParens": "always",
 "bracketSpacing": true,
 "insertPragma": false,
 "printWidth": 80,
 "proseWrap": "preserve",
 "quoteProps": "as-needed",
 "requirePragma": false,
 "semi": true,
 "singleQuote": true,
 "tabWidth": 3,
 "trailingComma": "all",
 "useTabs": true,
 "endOfLine": "auto",
  "overrides": [
    {
      "files": ".prettierrc",
      "options": { "parser": "typescript" }
    }
  ]
}